window.env = {
  name : '{{ key "indystardevelopment/thefineryears/production/APP_ENV"}}',
  webserviceApiKey: '{{ key "indystardevelopment/thefineryears/production/WEBSERVICE_API_KEY"}}',
  omniture : {
    reportingEnabled: false,
    defaults : {
      section: 'sponsor-story/dev',
      extension: null,
      showDevice: true,
      debugTransport: false
    }
  },
  googleMapsJSAPIKey: '{{ key "indystardevelopment/thefineryears/production/googleMapsJSAPIKey"}}',
  googleAnalytics: {
    UACode: '{{ key "indystardevelopment/thefineryears/production/googleAnalytics_UACode"}}', // indystardev group
    domainName: '{{ key "indystardevelopment/thefineryears/production/googleAnalytics_domainName"}}'
  },
  facebook: {
    appId: '{{ key "indystardevelopment/thefineryears/production/facebook_appId"}}'
  },
};
/**
 * URL-specific Facebook App IDs
 */
window.facebookAppIDs = {{ key "indystardevelopment/thefineryears/production/facebookAppIDs"}};
for (var key in window.facebookAppIDs) {
  if (window.location.href.indexOf(key) !== -1) {
    window.env.facebook.appId = window.facebookAppIDs[key];
    break;
  }
}
